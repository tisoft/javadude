   </extension>
   <extension point="org.eclipse.ui.workingSets">
      <workingSet
            icon="icons/workset.gif"
            id="com.hcrest.classpath.regexWorkingSet"
            name="Regular Expression Working Set"
            pageClass="com.javadude.workingsets.RegExWorkingSetPage"
            updaterClass="com.javadude.workingsets.RegExWorkingSetUpdater">
      </workingSet>
   </extension>
</plugin>
