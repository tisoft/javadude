      <startup
            class="com.javadude.workingsets.DummyStartup">
      </startup>
   </extension>
   <extension point="org.eclipse.ui.workingSets">
      <workingSet
            icon="icons/workset.gif"
            id="com.hcrest.classpath.natureWorkingSet"
            name="Nature Working Set"
            pageClass="com.javadude.workingsets.NatureWorkingSetPage"
            updaterClass="com.javadude.workingsets.NatureWorkingSetUpdater">
